{
  "name": "analytics-benson",
  "version": "1.0.22",
  "description": "Allows a dev to keep track of their websites analytics and statistics",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "index.js",
    "analytics.js",
    "analytics.min.js",
    "index.esm.js"
  ],
  "type": "module",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    "transform-modules-commonjs"
  ],
  "scripts": {
    "build": "babel index.js --out-file dist/index.js && babel index.js --out-file dist/index.esm.js --env-name esm && cp index.esm.js dist/",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tm-LBenson/analytics-package.git"
  },
  "keywords": [
    "analytics",
    "statistics",
    "website",
    "tracking",
    "monitoring"
  ],
  "author": "Lewis Benson",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2"
  }
}
